{
  "name": "bar.io_super_admin",
  "version": "0.1.0",
  "description": "bar.io - super admin site",
  "homepage": "",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "author": "Lemoncode",
  "main": "index.js",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "webpack serve --config ./config/webpack/dev.js",
    "build": "run-p -l type-check build:prod",
    "build:prod": "npm run clean && webpack --config ./config/webpack/prod.js",
    "analyze": "npm run clean && npm run type-check && webpack --config ./config/webpack/analyze.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf dist",
    "test": "jest -c ./config/test/jest.js --verbose",
    "test:watch": "npm run test -- --watchAll -i --no-cache",
    "test:e2e": "npm run start -- start:e2e",
    "start:e2e": "cypress open",
    "test:e2e:ci": "npm run start -- -r start:e2e:ci",
    "start:e2e:ci": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@lemoncode/fonk": "^1.5.1",
    "@lemoncode/fonk-formik": "^4.0.1",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "emotion": "^10.0.27",
    "formik": "^2.2.6",
    "immer": "^8.0.0",
    "jspdf": "^2.3.0",
    "lodash.merge": "^4.6.2",
    "qrcode": "^1.4.4",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/jspdf": "^1.3.3",
    "@types/qrcode": "^1.3.5",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-emotion": "^10.0.33",
    "cypress": "^5.5.0",
    "dotenv-webpack": "^5.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.2"
  }
}
